  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch ./src/**/*.ts --exec ts-node ./src/app.ts",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "migration:generate": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js  migration:generate ./src/.database/pg/migration/ --dataSource ./src/.database/pg/migration_src.ts",
    "migration:run": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js  -d  ./src/.database/pg/migration_src.ts  migration:run",
    "migration:revert": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js  -d  ./src/.database/pg/migration_src.ts  migration:revert",
    "mongo-migration:generate": "npx migrate-mongo create --file ./migrate-mongo-config.js",
    "mongo-migration:up": "npx migrate-mongo up --file ./migrate-mongo-config.js",
    "mongo-migration:down": "npx migrate-mongo down --file ./migrate-mongo-config.js",
    "mongo-migration:status": "npx migrate-mongo status --file ./migrate-mongo-config.js",
    "test_jest": "jest",
    "test:watch": "npm test -- --watch"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 150,
    "semi": false,
    "singleQuote": false,
    "bracketSpacing": true,
    "bracketSameLine": true,
    "arrowParens": "always",
    "singleAttributePerLine": true
  },
